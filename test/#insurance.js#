e\x74\x22\x3a\x7b\x22\x73\x63\x68\x65\x64\x75\x6c\x65\x64\x45\x71\x75\x69\x70\x6d\x65\x6e\x74\x49\x61\x74\x61\x43\x6f\x64\x65\x22\x3a\x22\x33\x32\x30\x22\x2c\x22\x61\x63\x74\x75\x61\x6c\x45\x71\x75\x69\x70\x6d\x65\x6e\x74\x49\x61\x74\x61\x43\x6f\x64\x65\x22\x3a\x22\x33\x32\x30\x22\x7d\x7d\x7d\xc5\x85\xfd\x47\x90\x8f\x6f\x8d\x5b\x1c\xb3\x73\x75\xd4\x15\xa8';
    var insuranceID = 1000;

    return Insurance.deployed().then(function(instance) {
      app = instance;
      return expectThrow(app.resolveContract(insuranceID, flightProof));
    });
    // .then(function() {
    //   return app.allInsuranceCovers(insuranceID);
    // }).then(function(contract){
    //   assert.equal(contract[8], false, "contract deleted is not set correctly");
    // });
  });

  // negative cancelInsuranceContract
  it("A user should not be able to cancel a insurance contract that has been filled", function(){
    var proposer = accounts[0];
    var insuranceID = 1000;

    return Insurance.deployed().then(function(instance) {
      app = instance;
      return expectThrow(app.cancelInsuranceContract(insuranceID, {from: proposer}));
    });
  });

  // resolveContract
  it("A contract should be resolved if the flight was not cancelled", function() {
    // var app;
    var flightProof =
    "\x20\x00\x10\x00\xff\xff\x03\x00\x89\xdc\x8d\xc0\x2e\x69\x05\x00\xf1\x1d\x9f\xc0\x2e\x69\x05\x00\x40\x00\x00\x00\x04\x00\x00\x00\xd1\x53\x97\x69\x50\xee\x15\xc4\x8a\x8b\x29\x3f\x99\x6c\x27\x59\x2e\x21\x57\x52\x55\xc4\xee\x20\xc3\x1b\x8b\xce\xa4\x9e\x4d\x6e\xfa\x12\x67\x7b\x36\x02\xb5\x83\x9d\x1a\xac\x03\x7b\x9e\x71\x43\xc7\x82\xe7\xf4\x05\x46\x08\xf0\xb9\x0f\xd8\xfd\x90\xde\xf4\x94\x02\xdb\x00\x00\x47\x45\x54\x20\x2f\x70\x72\x6f\x78\x79\x2e\x70\x79\x3f\x75\x72\x6c\x3d\x68\x74\x74\x70\x73\x25\x33\x41\x2f\x2f\x61\x70\x69\x2e\x66\x6c\x69\x67\x68\x74\x73\x74\x61\x74\x73\x2e\x63\x6f\x6d\x2f\x66\x6c\x65\x78\x2f\x66\x6c\x69\x67\x68\x74\x73\x74\x61\x74\x75\x73\x2f\x72\x65\x73\x74\x2f\x76\x32\x2f\x6a\x73\x6f\x6e\x2f\x66\x6c\x69\x67\x68\x74\x2f\x73\x74\x61\x74\x75\x73\x2f\x39\x35\x34\x37\x32\x30\x30\x31\x35\x25\x33\x46\x61\x70\x70\x49\x64\x25\x33\x44\x31\x64\x34\x37\x65\x30\x34\x32\x25\x32\x36\x61\x70\x70\x4b\x65\x79\x25\x33\x44\x61\x35\x62\x66\x65\x32\x61\x32\x35\x61\x31\x35\x66\x30\x35\x64\x62\x31\x35\x38\x38\x30\x38\x34\x65\x61\x32\x35\x36\x34\x37\x37\x20\x48\x54\x54\x50\x2f\x31\x2e\x31\x0d\x0a\x48\x6f\x73\x74\x3a\x20\x74\x6c\x73\x2d\x6e\x2e\x6f\x72\x67\x0d\x0a\x43\x6f\x6e\x6e\x65\x63\x74\x69\x6f\x6e\x3a\x20\x6b\x65\x65\x70\x2d\x61\x6c\x69\x76\x65\x0d\x0a\x0d\x0a\xe3\x13\x3c\xee\xfa\x19\x5c\xc1\x57\x73\x02\xe1\xc5\x1e\xd7\x0a\x02\xf9\x00\x01\x48\x54\x54\x50\x2f\x31\x2e\x31\x20\x32\x30\x30\x20\x4f\x4b\x0d\x0a\x44\x61\x74\x65\x3a\x20\x46\x72\x69\x2c\x20\x30\x36\x20\x41\x70\x72\x20\x32\x30\x31\x38\x20\x31\x34\x3a\x32\x30\x3a\x30\x33\x20\x47\x4d\x54\x0d\x0a\x53\x65\x72\x76\x65\x72\x3a\x20\x41\x70\x61\x63\x68\x65\x2f\x32\x2e\x34\x2e\x36\x20\x28\x52\x65\x64\x20\x48\x61\x74\x20\x45\x6e\x74\x65\x72\x70\x72\x69\x73\x65\x20\x4c\x69\x6e\x75\x78\x29\x20\x6d\x6f\x64\x5f\x6e\x73\x73\x2f\x31\x2e\x30\x2e\x31\x34\x20\x4e\x53\x53\x2f\x33\x2e\x33\x30\x2e\x31\x20\x4f\x70\x65\x6e\x53\x53\x4c\x2f\x31\x2e\x30\x2e\x32\x6b\x2d\x66\x69\x70\x73\x20\x50\x48\x50\x2f\x35\x2e\x34\x2e\x31\x36\x0d\x0