5\x6e\x77\x61\x79\x41\x72\x72\x69\x76\x61\x6c\x22\x3a\x7b\x22\x64\x61\x74\x65\x4c\x6f\x63\x61\x6c\x22\x3a\x22\x32\x30\x31\x38\x2d\x30\x34\x2d\x30\x35\x54\x30\x39\x3a\x30\x37\x3a\x30\x30\x2e\x30\x30\x30\x22\x2c\x22\x64\x61\x74\x65\x55\x74\x63\x22\x3a\x22\x32\x30\x31\x38\x2d\x30\x34\x2d\x30\x35\x54\x30\x38\x3a\x30\x37\x3a\x30\x30\x2e\x30\x30\x30\x5a\x22\x7d\x2c\x22\x61\x63\x74\x75\x61\x6c\x52\x75\x6e\x77\x61\x79\x41\x72\x72\x69\x76\x61\x6c\x22\x3a\x7b\x22\x64\x61\x74\x65\x4c\x6f\x63\x61\x6c\x22\x3a\x22\x32\x30\x31\x38\x2d\x30\x34\x2d\x30\x35\x54\x30\x39\x3a\x30\x37\x3a\x30\x30\x2e\x30\x30\x30\x22\x2c\x22\x64\x61\x74\x65\x55\x74\x63\x22\x3a\x22\x32\x30\x31\x38\x2d\x30\x34\x2d\x30\x35\x54\x30\x38\x3a\x30\x37\x3a\x30\x30\x2e\x30\x30\x30\x5a\x22\x7d\x7d\x2c\x22\x63\x6f\x64\x65\x73\x68\x61\x72\x65\x73\x22\x3a\x5b\x7b\x22\x66\x73\x43\x6f\x64\x65\x22\x3a\x22\x41\x59\x22\x2c\x22\x66\x6c\x69\x67\x68\x74\x4e\x75\x6d\x62\x65\x72\x22\x3a\x22\x35\x36\x36\x37\x22\x2c\x22\x72\x65\x6c\x61\x74\x69\x6f\x6e\x73\x68\x69\x70\x22\x3a\x22\x4c\x22\x7d\x2c\x7b\x22\x66\x73\x43\x6f\x64\x65\x22\x3a\x22\x42\x41\x22\x2c\x22\x66\x6c\x69\x67\x68\x74\x4e\x75\x6d\x62\x65\x72\x22\x3a\x22\x31\x35\x38\x37\x22\x2c\x22\x72\x65\x6c\x61\x74\x69\x6f\x6e\x73\x68\x69\x70\x22\x3a\x22\x4c\x22\x7d\x2c\x7b\x22\x66\x73\x43\x6f\x64\x65\x22\x3a\x22\x49\x42\x22\x2c\x22\x66\x6c\x69\x67\x68\x74\x4e\x75\x6d\x62\x65\x72\x22\x3a\x22\x39\x30\x37\x33\x22\x2c\x22\x72\x65\x6c\x61\x74\x69\x6f\x6e\x73\x68\x69\x70\x22\x3a\x22\x4c\x22\x7d\x2c\x7b\x22\x66\x73\x43\x6f\x64\x65\x22\x3a\x22\x4c\x59\x22\x2c\x22\x66\x6c\x69\x67\x68\x74\x4e\x75\x6d\x62\x65\x72\x22\x3a\x22\x38\x31\x38\x33\x22\x2c\x22\x72\x65\x6c\x61\x74\x69\x6f\x6e\x73\x68\x69\x70\x22\x3a\x22\x4c\x22\x7d\x2c\x7b\x22\x66\x73\x43\x6f\x64\x65\x22\x3a\x22\x4d\x48\x22\x2c\x22\x66\x6c\x69\x67\x68\x74\x4e\x75\x6d\x62\x65\x72\x22\x3a\x22\x39\x35\x32\x39\x22\x2c\x22\x72\x65\x6c\x61\x74\x69\x6f\x6e\x73\x68\x69\x70\x22\x3a\x22\x4c\x22\x7d\x5d\x2c\x22\x64\x65\x6c\x61\x79\x73\x22\x3a\x7b\x22\x64\x65\x70\x61\x72\x74\x75\x72\x65\x47\x61\x74\x65\x44\x65\x6c\x61\x79\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x31\x32\x2c\x22\x64\x65\x70\x61\x72\x74\x75\x72\x65\x52\x75\x6e\x77\x61\x79\x44\x65\x6c\x61\x79\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x34\x39\x2c\x22\x61\x72\x72\x69\x76\x61\x6c\x47\x61\x74\x65\x44\x65\x6c\x61\x79\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x31\x34\x2c\x22\x61\x72\x72\x69\x76\x61\x6c\x52\x75\x6e\x77\x61\x79\x44\x65\x6c\x61\x79\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x31\x36\x7d\x2c\x22\x66\x6c\x69\x67\x68\x74\x44\x75\x72\x61\x74\x69\x6f\x6e\x73\x22\x3a\x7b\x22\x73\x63\x68\x65\x64\x75\x6c\x65\x64\x42\x6c\x6f\x63\x6b\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x34\x35\x30\x2c\x22\x62\x6c\x6f\x63\x6b\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x34\x35\x32\x2c\x22\x73\x63\x68\x65\x64\x75\x6c\x65\x64\x41\x69\x72\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x34\x32\x31\x2c\x22\x61\x69\x72\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x33\x38\x38\x2c\x22\x73\x63\x68\x65\x64\x75\x6c\x65\x64\x54\x61\x78\x69\x4f\x75\x74\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x32\x30\x2c\x22\x74\x61\x78\x69\x4f\x75\x74\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x35\x37\x2c\x22\x73\x63\x68\x65\x64\x75\x6c\x65\x64\x54\x61\x78\x69\x49\x6e\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x39\x2c\x22\x74\x61\x78\x69\x49\x6e\x4d\x69\x6e\x75\x74\x65\x73\x22\x3a\x37\x7d\x2c\x22\x61\x69\x72\x70\x6f\x72\x74\x52\x65\x73\x6f\x75\x72\x63\x65\x73\x22\x3a\x7b\x22\x64\x65\x70\x61\x72\x74\x75\x72\x65\x54\x65\x72\x6d\x69\x6e\x61\x6c\x22\x3a\x22\x41\x22\x2c\x22\x64\x65\x70\x61\x72\x74\x75\x72\x65\x47\x61\x74\x65\x22\x3a\x22\x31\x37\x22\x2c\x22\x61\x72\x72\x69\x76\x61\x6c\x54\x65\x72\x6d\x69\x6e\x61\x6c\x22\x3a\x22\x33\x22\x2c\x22\x61\x72\x72\x69\x76\x61\x6c\x47\x61\x74\x65\x22\x3a\x22\x32\x31\x22\x2c\x22\x62\x61\x67\x67\x61\x67\x65\x22\x3a\x22\x30\x31\x22\x7d\x2c\x22\x66\x6c\x69\x67\x68\x74\x45\x71\x75\x69\x70\x6d\x65\x6e\x74\x22\x3a\x7b\x22\x73\x63\x68\x65\x64\x75\x6c\x65\x64\x45\x71\x75\x69\x70\x6d\x65\x6e\x74\x49\x61\x74\x61\x43\x6f\x64\x65\x22\x3a\x22\x33\x33\x33\x22\x2c\x22\x74\x61\x69\x6c\x4e\x75\x6d\x62\x65\x72\x22\x3a\x22\x4e\x32\x37\x30\x41\x59\x22\x7d\x7d\x7d\x36\xac\x3a\x3e\x30\xb3\xd0\x6c\x15\x5a\xc2\x6b\xe9\xd1\x2f\x50";
    var insuranceID = 1000;
    var contributor1BalanceBefore = web3.eth.getBalance(accounts[1]).toNumber();
    var contributor2BalanceBefore = web3.eth.getBalance(accounts[2]).toNumber();


    return Insurance.deployed().then(function(instance) {
      app = instance;
      let event = app.Status(function(error, result) {
      if (!error)
          status = result.args.status.toNumber();
          console.log("status: ", status);
      });
      return app.resolveContract(insuranceID, flightProof);
    }).then(function() {
      return app.getInsuranceContributions.call(insuranceID);
    }).then(function(out) {
      contributions = out;
      // console.log("contributions[0]: ", contributions[0].toNumber());
      // console.log("contributions[1]: ", contributions[1].toNumber());
      return app.allInsuranceCovers(insuranceID);
    }).then(function(contract){
      contributor1BalanceAfter = web3.eth.getBalance(accounts[1]).toNumber();
      contributor2BalanceAfter = web3.eth.getBalance(accounts[2]).toNumber();
      // console.log("contributor1BalanceAfter: ", contributor1BalanceAfter);
      // console.log("contributor2BalanceAfter: ", contributor2BalanceAfter);
      totalCoverAmount = contract[3].toNumber();
      premiumAmount = contract[5].toNumber();
      // console.log("totalCoverAmount: ", totalCoverAmount);
      // console.log("premiumAmount: ", premiumAmount);
      assert.equal(contract[8], true, "contract deleted is not set correctly");
      assert.equal(contributor1BalanceAfter - contributor1BalanceBefore,
      contributions[0].toNumber() + (contributions[0].toNumber()*premiumAmount)/totalCoverAmount, "contributor1 funds are incorrect");
      assert.equal(contributor2BalanceAfter - contributor2BalanceBefore,
      contributions[1].toNumber() + (contributions[1].toNumber()*premiumAmount)/totalCoverAmount, "contributor2 funds are incorrect");
    });
  });

  // Cancelling bet

  // proposeInsuranceCover
  it("A user should be able to propose an insurance contract, which can be retrieved", function() {
    // var app;
    var proposer = accounts[0];
    var flightProof =
